{
    "name": "littlegreenman/linodev4",
    "description": "A Laravel library for interacting with the Linode API",
    "license": "MIT",
    "authors": [
        {
            "name": "Elliot Ali",
            "email": "elliot@lgm.ltd",
            "homepage": "https://lgm.ltd"
        }
    ],
    "homepage": "https://github.com/little-green-man/linodev4",
    "keywords": ["Laravel", "LinodeV4", "Linode"],
    "require": {
        "illuminate/support": "~7|~8",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "LittleGreenMan\\LinodeV4\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LittleGreenMan\\LinodeV4\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LittleGreenMan\\LinodeV4\\LinodeV4ServiceProvider"
            ],
            "aliases": {
                "LinodeV4": "LittleGreenMan\\LinodeV4\\Facades\\LinodeV4"
            }
        }
    }
}
